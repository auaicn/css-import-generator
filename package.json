  "homepage": "https://github.com/auaicn/css-import-generator#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/auaicn/css-import-generator.git"
  },
  "bugs": {
    "url": "https://github.com/auaicn/css-import-generator/issues"
  },
  "scripts": {
    "type": "npx tsc"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "typescript": "^4.7.4",
    "prettier": "2.3.2"
  },
  "dependencies": {
    "find-package-json": "^1.2.0"
  }
}
